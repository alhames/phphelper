{
  "name": "alhames/phphelper",
  "type": "library",
  "description": "PHP Helpers",
  "homepage": "https://github.com/alhames/phphelper",
  "license": "MIT",
  "authors": [
    {
      "name": "Pavel Logachev",
      "email": "alhames@mail.ru",
      "homepage": "https://alhames.ru/"
    }
  ],
  "require": {
    "php": "^7.1",
    "ext-json": "*",
    "ext-mbstring": "*"
  },
  "require-dev": {
    "guzzlehttp/guzzle": "^6.0",
    "phpunit/phpunit": "^7.5",
    "symfony/yaml": "^4.4 || ^5.0"
  },
  "suggest": {
    "ext-fileinfo": "FileInfo is required for PhpHelper\\Image\\Image",
    "ext-gd": "GD is required for PhpHelper\\Image\\EditableImage",
    "guzzle/guzzle": "Guzzle is required for PhpHelper\\Image\\Image"
  },
  "support": {
    "issues": "https://github.com/alhames/phphelper/issues"
  },
  "autoload": {
    "psr-4": {
      "PhpHelper\\": "src/"
    },
    "files": ["functions.php"]
  },
  "autoload-dev": {
    "psr-4": {
      "PhpHelper\\Tests\\": "tests/"
    }
  }
}
